    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug Danbooru default",
            "cargo": {
                "args": [
                    "build",
                    "--bin=imageboard_downloader",
                    "--package=imageboard_downloader"
                ],
                "filter": {
                    "name": "imageboard_downloader",
                    "kind": "bin"
                }
            },
            "args": [
                "delphox"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug Gelbooru default",
            "cargo": {
                "args": [
                    "build",
                    "--bin=imageboard_downloader",
                    "--package=imageboard_downloader"
                ],
                "filter": {
                    "name": "imageboard_downloader",
                    "kind": "bin"
                }
            },
            "args": [
                "-i",
                "gelbooru",
                "kroos_(arknights)"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Test danbooru post limit",
            "cargo": {
                "args": [
                    "build",
                    "--bin=imageboard_downloader",
                    "--package=imageboard_downloader"
                ],
                "filter": {
                    "name": "imageboard_downloader",
                    "kind": "bin"
                }
            },
            "args": [
                "kroos_(arknights)",
                "-l",
                "10"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Test R34 post limit",
            "cargo": {
                "args": [
                    "build",
                    "--bin=imageboard_downloader",
                    "--package=imageboard_downloader"
                ],
                "filter": {
                    "name": "imageboard_downloader",
                    "kind": "bin"
                }
            },
            "args": [
                "kroos_(arknights)",
                "-l",
                "50",
                "-i",
                "rule34"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}